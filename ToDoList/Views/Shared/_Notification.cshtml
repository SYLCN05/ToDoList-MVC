@if (TempData["Created"] != null)
{
    <div class="alert alert-dismissible alert-success">
        <button type="button" class="btn-close" data-bs-dismiss="alert"></button>
        <strong>@TempData["Created"]</strong>
    </div>
}

@if (TempData["Edited"] != null)
{
    <div class="alert alert-dismissible alert-info">
        <button type="button" class="btn-close" data-bs-dismiss="alert"></button>
        <strong>@TempData["Edited"]</strong>
    </div>
}

@if (TempData["Deleted"] != null)
{
    <div class="alert alert-danger alert-success">
        <button type="button" class="btn-close" data-bs-dismiss="alert"></button>
        <strong>@TempData["Deleted"]</strong>
    </div>
}
@if (TempData["Cloned"] != null)
{
    <div class="alert alert-dismissible alert-primary">
        <button type="button" class="btn-close" data-bs-dismiss="alert"></button>
        <strong>@TempData["Cloned"]</strong>

    </div>
}
@if(TempData["Error"] != null)
{
    <div class=" alert alert-danger alert-warning">
        <button type="button" class="btn-close" data-bs-dismiss="alert"></button>
        <strong>@TempData["Error"]</strong>
    </div>
}